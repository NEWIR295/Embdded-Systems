/*AVR STD Libraries*/
#include <avr/io.h>
#include <util/delay.h>
/*main function*/
int main(void){
	/*initialization of pin direction*/
	DDRC |=(1<<2); /*set LED PIN 1 to be output*/

	while(1){
		PORTC |=(1<<2);
	}
	return 0;
}
